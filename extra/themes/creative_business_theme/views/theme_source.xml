<?xml version="1.0" encoding="utf-8"?>
<incore>
    <data>

        <template id="template_inherit" name="template_inherit" inherit_id="website.assets_frontend" >
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/creative_business_theme/static/src/css/owl.carousel.css"/>
                <link rel="stylesheet" href="/creative_business_theme/static/src/css/owl.theme.css"/>
                <link rel="stylesheet" href="/creative_business_theme/static/src/css/owl.transitions.css"/>
                <link rel="stylesheet" href="/creative_business_theme/static/src/css/style.css"/>
            </xpath>
        </template>

        <template id="clean_script" inherit_id="website.assets_frontend" name="Clean scripts">
            <xpath expr="/t" position="inside">
                <script src="/creative_business_theme/static/src/js/website_subscribe.js"></script>
                <script src="/creative_business_theme/static/src/js/subscribe_contactus.js"></script>
                <script src="/creative_business_theme/static/src/js/jquery.easing.min.js"></script>
                <script src="/creative_business_theme/static/src/js/jquery.appear.js"></script>
                <script src="/creative_business_theme/static/src/js/jquery.nicescroll.min.js"></script>
                <script src="/creative_business_theme/static/src/js/jquery.countTo.js"></script>
                <script src="/creative_business_theme/static/src/js/jquery.shuffle.modernizr.js"></script>
                <script src="/creative_business_theme/static/src/js/jquery.shuffle.js"></script>
                <script src="/creative_business_theme/static/src/js/owl.carousel.js"></script>
                <script src="/creative_business_theme/static/src/js/script.js"></script>
            </xpath>
        </template>

        <template id="top_header" name="Main layout" inherit_id="portal.frontend_layout">
            <xpath expr="//header//ul[@id='top_menu']/li[hasclass('divider')]" position="replace">
            </xpath>
        </template>

        <template id="top_header_signin" inherit_id="website.layout" name="top_header">
            <xpath expr="//header//ul[@id='top_menu']/li[3]" position="replace">
            </xpath>
            <xpath expr="//main" position="after">
                <a href="#home" class="cd-top">
                    Top
                </a>
            </xpath>
        </template>

    </data>
</incore>
