<?xml version="1.0"?>
<incore>
		<record id="pos_config_view_form_inherit" model="ir.ui.view">
			<field name="name">pos.config.form.view</field>
			<field name="model">pos.config</field>
			<field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
			<field name="arch" type="xml">
				<xpath expr="//div[4]" position="after">
					<h2>Allow To Print Kitchen Order Receipt</h2>
					<div class="row mt16 o_settings_container">
						<div class="col-md-6 o_setting_box">
							<div class="o_setting_left_pane">
								<field name="print_kitchen_receipt"/>
							</div>
							<div class="o_setting_right_pane">
								<label for="print_kitchen_receipt"/>
							</div>
						</div>
					</div>
				</xpath>
			</field>
		</record>
</incore>