<?xml version="1.0" encoding="utf-8"?>
<incore>
	<data>
		<record id="sh_pos_res_config_settings_enable_quotation_load" model="ir.ui.view">
	        <field name="name">pos.config.settings.enable.quotation.load</field>
	        <field name="model">pos.config</field>
	        <field name="inherit_id" ref="point_of_sale.pos_config_view_form" />
	        <field name="arch" type="xml">
	            <xpath expr="//h2[@name='order']" position="before">
	                <h2>Enable Quotation Load Feature</h2>
	                <div class="row mt16 o_settings_container">
	                    <div class="col-xs-6 col-md-6 o_setting_box">
	                        <div class="o_setting_left_pane">
	                            <field name="sh_enable_quotation_load" />
	                        </div>
	                        <div class="o_setting_right_pane">
	                            <label for="sh_enable_quotation_load" />
	                        </div>
	                    </div>
	                </div>
	            </xpath>
	        </field>
    	</record>
		<record id="pos_order_inherit_form_view" model="ir.ui.view">
			<field name="name">pos.order.inherit.form.view</field>
			<field name="model">pos.order</field>
			<field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
			<field name="arch" type="xml">
				<field name="pos_reference" position="after">
					<field name="sale_order_id"/>
				</field>
			</field>
		</record>
	</data>
</incore>