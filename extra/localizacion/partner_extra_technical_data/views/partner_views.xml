<incore>
    <data>
        <record id="res_partner_form_view_inherit_partner_extra_technical_data" model="ir.ui.view">
            <field name="name">res.partner.form.inherit.partner_extra_technical_data</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[last()]" position="after">
                    <page string="Informacion Tecnica">
                        <group>
                            <field name="start_date"/>
                            <field name="domain_name"/>
                            <field name="user_name"/>
                            <field name="system_version"/>  
                            <field name="server_id"/>
                            <field name="product_ids" widget="many2many_tags"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</incore>
